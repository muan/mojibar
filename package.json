{
  "name": "mojibar",
  "version": "2.6.2",
  "description": "A menubar app adaptation of Emoji searcher.",
  "main": "index.js",
  "dependencies": {
    "electron-is-dev": "^0.1.2",
    "emojilib": "2.2.3",
    "honkling-node": "file:../honkling-node",
    "jquery": "^3.3.1",
    "mac-open": "^0.1.3",
    "menubar": "^5.1.0",
    "osx": "^0.1.0",
    "osx-vol": "^2.0.0",
    "ps-list": "^6.0.0",
    "visualizer.js": "^3.0.0",
    "youtube-search": "^1.1.3"
  },
  "devDependencies": {
    "electron": "^3.0.10",
    "electron-packager": "^12.2.0",
    "standard": "^12.0.1"
  },
  "scripts": {
    "start": "electron . --enable-logging",
    "build": "electron-packager . Mojibar --platform=darwin --arch=x64 --overwrite --icon=mojibar",
